    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^4.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
